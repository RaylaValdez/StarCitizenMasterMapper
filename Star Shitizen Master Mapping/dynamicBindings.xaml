<UserControl x:Name="uiBindingsForSelectedCategory" x:Class="Star_Shitizen_Master_Mapping.dynamicBindings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Star_Shitizen_Master_Mapping"
             mc:Ignorable="d">
    <Grid x:Name="uiSubCatergoryBindingsGrid">
        <Rectangle Stroke="#FF3A7187" Fill="#C90F2C3E"/>
        <WrapPanel x:Name="uiSubCatergoryBindingsWrapPanel" Margin="0,10,0,958" HorizontalAlignment="Center" Width="580"/>
        <Grid x:Name="uiSubCatergoryBindingsInnerGrid" Margin="0,99,0,0">
            <ScrollViewer x:Name="uiCatergoryBindingsScroll" VerticalScrollBarVisibility="Auto">
                <StackPanel x:Name="uiCatergoryBindingsStack"/>
            </ScrollViewer>
        </Grid>

    </Grid>
</UserControl>
